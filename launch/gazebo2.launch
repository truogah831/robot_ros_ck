<launch>
  <include
    file="$(find gazebo_ros)/launch/empty_world.launch" />
  <node
    name="tf_footprint_base"
    pkg="tf"
    type="static_transform_publisher"
    args="0 0 0 0 0 0 base_link base_footprint 40" />
  <node
    name="spawn_model"
    pkg="gazebo_ros"
    type="spawn_model"
    args="-file $(find tn5)/urdf/tn5.urdf -urdf -model tn5"
    output="screen" />
  <node
    name="fake_joint_calibration"
    pkg="rostopic"
    type="rostopic"
    args="pub /calibrated std_msgs/Bool true" />

  <rosparam file="$(find tn5)/config/controller.yaml" command="load" />

  <node
      name="controller_spawner"
      pkg="controller_manager"
      type="spawner"
      args="khau1_joint_position_controller xoay_joint_position_controller"
      output="screen" />
</launch>



